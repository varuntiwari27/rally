{% set flavor_name = flavor_name or "m1.tiny" %}
{
  "NovaServers.boot_server_associate_and_dissociate_floating_ip": [
    {
      "runner": {
        "type": "constant",
        "concurrency": 2,
        "times": 5
      },
      "args": {
        "flavor": {
          "name": "{{flavor_name}}"
        },
        "image": {
          "name": "^cirros.*uec$"
        }
      },
      "context": {
        "users": {
          "users_per_tenant": 2,
          "tenants": 3
        },
        "network": {}
      },
      "sla": {
        "failure_rate": {
          "max": 0
        }
      }
    }
  ]
}
